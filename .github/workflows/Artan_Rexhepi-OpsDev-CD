name: OpsDev-CI
on:
  push:
    branches:
      - main
  pull_request: 
    branches:
      - main
  workflow_dispatch:
jobs:
  build:
    runs-on: ubuntu-latest

    strategy:
      matrix:
        node-version: [10.x, 12.x, 14.x, 15.x]
    steps:
      - uses: actions/checkout@v3
   
      - name: Docker Login
        uses: docker/login-action@v2.1.0
        with:
        
      - name: Build and push Docker images
        uses: docker/build-push-action@v4.0.0
        
        
